  "description": "Oppijan raamit ",
  "license": "EUPL, Version 1.1",
  "homepage": "https://github.com/Opetushallitus/oppija-raamit",
  "private": true,
  "scripts": {
    "dev": "npx webpack serve --progress --config build/webpack.dev.conf.cjs --color",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "build": "npm run templates && NODE_ENV=production node build/build.mjs",
    "templates": "rm -rf static/html/ && ./node_modules/static-i18n/bin/static-i18n.js -l fi -i fi -i sv -i en --outputDir static/html/ --outputDefault __lng__/__file__ --localesPath src/locales/ src/templates/"
  },
